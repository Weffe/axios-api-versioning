{
  "name": "axios-api-versioning",
  "author": "Rogelio Negrete (Weffe)",
  "version": "2.1.0",
  "description": "Adds api versioning for axios",
  "main": "dist/index.js",
  "module": "dist/axios-api-versioning.js",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "sandbox": "tsc -p sandbox/tsconfig.sandbox.json",
    "clean": "rimraf dist/",
    "release": "standard-version",
    "docs": "docsify serve docs -o --port 2065"
  },
  "keywords": [
    "axios",
    "api versioning"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Weffe/axios-api-versioning"
  },
  "bugs": {
    "url": "https://github.com/Weffe/axios-api-versioning/issues"
  },
  "homepage": "https://github.com/Weffe/axios-api-versioning",
  "peerDependencies": {
    "axios": "^0.18.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "axios": "0.18.0",
    "axios-mock-adapter": "^1.16.0",
    "docsify-cli": "^4.3.0",
    "http-status-codes": "^1.3.2",
    "husky": "^2.4.1",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "standard-version": "^6.0.1",
    "tsdx": "^0.7.1",
    "tslib": "^1.10.0",
    "typescript": "^3.5.2"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
